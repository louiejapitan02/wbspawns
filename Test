        time[4] = data['loran']
        timer = 0
        counter = 0
        while timer < 5:

            if current_time.hour == int(time[timer][0:2]):
                if current_time.minute == int(time[0][3:5]) - 10:
                    print('loran na')
                else:
                    pass
            else:
                pass
            timer += 1

time.sleep(3)
